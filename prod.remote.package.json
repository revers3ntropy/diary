{
    "type": "module",
    "scripts": {
        "docker-build": "docker image build -t misc_3-image .",
        "docker-start": "docker container run --network=host --name=misc_3-container --restart=always -v misc_3-mount:/app -p 3021:3000 -d misc_3-image",
        "start": "docker rm misc_3-container; npm run docker-build; npm run docker-start",
        "stop-docker": "sudo systemctl stop docker; sudo systemctl stop docker.socket",
        "start-docker": "sudo systemctl start docker",
        "stop": "docker stop misc_3-container",
        "restart": "npm run stop; npm run start",
        "clear": "npm run stop; docker rm misc_3-container; docker rmi $(docker images -a -q); npm run stop-docker; npm run start-docker",
        "logs": "docker logs misc_3-container",
        "dump-logs": "npm run logs | tee --append server.log",
        "clear-logs": "sudo truncate -s 0 $(sudo docker inspect --format='{{.LogPath}}' misc_3-container)",
        "clear-log-file": "> server.log",
        "start-local": "cd ..; npm run build-server; cd server; npm run start",
        "setup": "npm run stop; npm run clear-logs; npm run clear-log-file; npm run make-mount",
        "make-mount": "docker volume create misc_3-mount"
    },
    "dependencies": {
        "@fontsource/fira-mono": "^4.5.0",
        "@rollup/plugin-replace": "^5.0.1",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/adapter-node": "^1.0.0-next.99",
        "@sveltejs/kit": "next",
        "@types/cookie": "^0.5.1",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "autoprefixer": "^10.4.12",
        "cookie": "^0.4.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "js-sha256": "^0.9.0",
        "less": "^4.1.3",
        "postcss": "^8.4.18",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.46.0",
        "svelte-check": "^2.7.1",
        "svelte-material-icons": "^2.0.4",
        "svelte-preprocess": "^4.10.7",
        "tslib": "^2.3.1",
        "typescript": "^4.7.4",
        "vite": "^3.1.0-beta.1",
        "mysql2": "^2.3.3",
        "svelte-notifications": "^0.9.98"
    }
}
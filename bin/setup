#!/bin/bash

rm -r ./.svelte-kit
rm -r ./node_modules
rm -r ./bin/node_modules
rm -r ./playwright-report
rm -r ./test-results
rm -r ./build
rm ./package-lock.json
rm ./bin/package-lock.json
rm ./general.log
rm ./testBackup.json

(
  export PLAYWRIGHT_SKIP_BROWSER_GC=1
  npm i -f
) & (
  cd ./bin || exit 1
  npm i
) & wait

npx playwright install
